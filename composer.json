{
  "name": "maczz/think-glide-8",
  "description": "ThinkPHP6 adapter for using Glide image manipulation library",
  "type": "library",
  "keywords": ["thinkphp", "glide", "think-glide", "thinkphp8"],
  "license": "MIT",
  "authors": [
    {
      "name": "fork from byron sampson",
      "email": "xiaobo.sun@qq.com"
    }
  ],
  "require": {
    "php": ">=8.0.0",
    "topthink/framework": "^8.0",
    "league/glide": "^2.3",
    "symfony/options-resolver": "^2.0|^3.0|^4.0"
  },
  "autoload": {
    "psr-4": {
      "think\\": "src/"
    },
    "files": [
      "src/helper.php"
    ]
  },
  "extra": {
    "think": {
      "config":{
        "glide": "src/config.php"
      }
    }
  }
}
